      <rdf:Description dc:date="2024-12-11 01:03:39 PM" dc:source="Camtasia,9.0.4,enu" dc:title="module11_1" tscDM:firstFrame="module11_1_First_Frame.png" tscDM:originId="67823403-5526-4B23-8647-20BF45EAEF59" tscDM:project="module11_1">
         <xmpDM:duration xmpDM:scale="1/1000" xmpDM:value="216100"/>
         <xmpDM:videoFrameSize stDim:unit="pixel" stDim:h="900" stDim:w="1416"/>
         <tsc:langName>
            <rdf:Bag>
               <rdf:li xml:lang="en-US">English</rdf:li></rdf:Bag>
         </tsc:langName>
         <xmpDM:Tracks>
            <rdf:Bag>
               <rdf:li>
                  <rdf:Description xmpDM:trackType="Caption" xmpDM:frameRate="f1000" xmpDM:trackName="Captioning" stFnt:fontFamily="Arial" tscDM:fontSize="30" tscDM:bgOpacity="0.750000" tscDM:position="overlay">
                     <xmpDM:markers>
                        <rdf:Seq>
                           <rdf:li><rdf:Description xmpDM:duration="10000" xmpDM:startTime="0" tscDM:valign="bottom" tscDM:halign="center"><xmpDM:name><rdf:Alt><rdf:li xml:lang="en-US">{\rtf1 Chào mừng bạn đến với module "Chống lại quá tải nhận thức". Đây là một phần \par trong loạt bốn phần về cách dạy để học sinh ghi nhớ nội dung và kỹ năng quan \par trọng.}</rdf:li></rdf:Alt></xmpDM:name></rdf:Description></rdf:li><rdf:li><rdf:Description xmpDM:duration="5000" xmpDM:startTime="10000" tscDM:valign="bottom" tscDM:halign="center"><xmpDM:name><rdf:Alt><rdf:li xml:lang="en-US">{\rtf1 Nhấp vào từng biểu tượng để xem mô tả ngắn gọn về các module trong loạt này.}</rdf:li></rdf:Alt></xmpDM:name></rdf:Description></rdf:li><rdf:li><rdf:Description xmpDM:duration="15000" xmpDM:startTime="30000" tscDM:valign="bottom" tscDM:halign="center"><xmpDM:name><rdf:Alt><rdf:li xml:lang="en-US">{\rtf1 Để bắt đầu module này, bạn sẽ tham gia vào một thí nghiệm ngắn, nơi một giáo \par viên sẽ giải thích một vài điều về thú mỏ vịt.}</rdf:li></rdf:Alt></xmpDM:name></rdf:Description></rdf:li><rdf:li><rdf:Description xmpDM:duration="6000" xmpDM:startTime="45000" tscDM:valign="bottom" tscDM:halign="center"><xmpDM:name><rdf:Alt><rdf:li xml:lang="en-US">{\rtf1 Thú mỏ vịt sống trong nước ngọt và là một trong năm loài thú đẻ trứng duy nhất. \par Bạn là một loài thú nhưng bạn không đẻ trứng.}</rdf:li></rdf:Alt></xmpDM:name></rdf:Description></rdf:li><rdf:li><rdf:Description xmpDM:duration="6000" xmpDM:startTime="51000" tscDM:valign="bottom" tscDM:halign="center"><xmpDM:name><rdf:Alt><rdf:li xml:lang="en-US">{\rtf1 Gà đẻ trứng nhưng không phải là thú. Bạn có nghĩ rằng gà có thể được coi là chim \par khi chúng chỉ có thể bay khoảng 10 giây? Tôi thì không.}</rdf:li></rdf:Alt></xmpDM:name></rdf:Description></rdf:li><rdf:li><rdf:Description xmpDM:duration="7000" xmpDM:startTime="57000" tscDM:valign="bottom" tscDM:halign="center"><xmpDM:name><rdf:Alt><rdf:li xml:lang="en-US">{\rtf1 Thú mỏ vịt có chân có màng giống như một người bạn tôi từng gặp ở đại học. Anh \par ấy là thành viên đội bơi lội và nghĩ rằng chân có màng mang lại lợi thế cho mình.}</rdf:li></rdf:Alt></xmpDM:name></rdf:Description></rdf:li><rdf:li><rdf:Description xmpDM:duration="9000" xmpDM:startTime="64000" tscDM:valign="bottom" tscDM:halign="center"><xmpDM:name><rdf:Alt><rdf:li xml:lang="en-US">{\rtf1 Thú mỏ vịt, số nhiều là thú mỏ vịt (platypods), sống ở Úc, dọc theo bờ biển phía \par đông của lục địa. Nói về Úc, một trong những bộ phim yêu thích của tôi là Crocodile \par Dundee.}</rdf:li></rdf:Alt></xmpDM:name></rdf:Description></rdf:li><rdf:li><rdf:Description xmpDM:duration="7000" xmpDM:startTime="73000" tscDM:valign="bottom" tscDM:halign="center"><xmpDM:name><rdf:Alt><rdf:li xml:lang="en-US">{\rtf1 Nếu bạn chưa xem, bạn nên xem thử. Tôi tự hỏi diễn viên Paul Hogan giờ đang ở \par đâu. Thú mỏ vịt chủ yếu sống về đêm và thường nhút nhát.}</rdf:li></rdf:Alt></xmpDM:name></rdf:Description></rdf:li><rdf:li><rdf:Description xmpDM:duration="6000" xmpDM:startTime="80000" tscDM:valign="bottom" tscDM:halign="center"><xmpDM:name><rdf:Alt><rdf:li xml:lang="en-US">{\rtf1 Một số nhà khoa học tin rằng não của người nhút nhát hoạt động khác với người \par hướng ngoại. Thú đẻ trứng được gọi là thú đơn huyệt.}</rdf:li></rdf:Alt></xmpDM:name></rdf:Description></rdf:li><rdf:li><rdf:Description xmpDM:duration="5000" xmpDM:startTime="86000" tscDM:valign="bottom" tscDM:halign="center"><xmpDM:name><rdf:Alt><rdf:li xml:lang="en-US">{\rtf1 Thú mỏ vịt trữ thức ăn trong má và lặn khi nổi lên mặt nước, giống như một con sóc \par nước.}</rdf:li></rdf:Alt></xmpDM:name></rdf:Description></rdf:li><rdf:li><rdf:Description xmpDM:duration="5000" xmpDM:startTime="91000" tscDM:valign="bottom" tscDM:halign="center"><xmpDM:name><rdf:Alt><rdf:li xml:lang="en-US">{\rtf1 Không biết Paul Hogan có họ hàng gì với đô vật Hulk Hogan không nhỉ?}</rdf:li></rdf:Alt></xmpDM:name></rdf:Description></rdf:li><rdf:li><rdf:Description xmpDM:duration="5000" xmpDM:startTime="96000" tscDM:valign="bottom" tscDM:halign="center"><xmpDM:name><rdf:Alt><rdf:li xml:lang="en-US">{\rtf1 Bạn nghĩ rằng mình nhớ được bao nhiêu thông tin về thú mỏ vịt?}</rdf:li></rdf:Alt></xmpDM:name></rdf:Description></rdf:li><rdf:li><rdf:Description xmpDM:duration="5000" xmpDM:startTime="101000" tscDM:valign="bottom" tscDM:halign="center"><xmpDM:name><rdf:Alt><rdf:li xml:lang="en-US">{\rtf1 Bạn nghĩ rằng mình sẽ nhớ được bao nhiêu thông tin này sau 10 ngày, thậm chí là \par 10 phút nữa?}</rdf:li></rdf:Alt></xmpDM:name></rdf:Description></rdf:li><rdf:li><rdf:Description xmpDM:duration="9000" xmpDM:startTime="106000" tscDM:valign="bottom" tscDM:halign="center"><xmpDM:name><rdf:Alt><rdf:li xml:lang="en-US">{\rtf1 Những gì bạn vừa trải nghiệm là quá tải nhận thức. Mặc dù đây là một ví dụ cực \par đoan, nhiều học sinh gặp phải điều này trong suốt năm học.}</rdf:li></rdf:Alt></xmpDM:name></rdf:Description></rdf:li><rdf:li><rdf:Description xmpDM:duration="15000" xmpDM:startTime="115000" tscDM:valign="bottom" tscDM:halign="center"><xmpDM:name><rdf:Alt><rdf:li xml:lang="en-US">{\rtf1 Quá tải nhận thức xảy ra khi giáo viên giải thích quá nhiều trong một khoảng thời \par gian ngắn, giới thiệu một chủ đề mới mà không kết nối với thông tin đã học trước \par đó, hoặc truyền đạt nội dung khó một cách không có tổ chức.}</rdf:li></rdf:Alt></xmpDM:name></rdf:Description></rdf:li><rdf:li><rdf:Description xmpDM:duration="11000" xmpDM:startTime="130000" tscDM:valign="bottom" tscDM:halign="center"><xmpDM:name><rdf:Alt><rdf:li xml:lang="en-US">{\rtf1 Trong module này, bạn sẽ học về quá tải nhận thức là gì, nguyên nhân gây ra nó, \par và ba chiến lược bạn có thể sử dụng trong lớp học để ngăn ngừa nó.}</rdf:li></rdf:Alt></xmpDM:name></rdf:Description></rdf:li><rdf:li><rdf:Description xmpDM:duration="8000" xmpDM:startTime="142000" tscDM:valign="bottom" tscDM:halign="center"><xmpDM:name><rdf:Alt><rdf:li xml:lang="en-US">{\rtf1 Quá tải nhận thức xảy ra khi giáo viên cung cấp quá nhiều thông tin và kích thích \par cho bộ nhớ làm việc của học sinh khiến họ không thể xử lý hết.}</rdf:li></rdf:Alt></xmpDM:name></rdf:Description></rdf:li><rdf:li><rdf:Description xmpDM:duration="3000" xmpDM:startTime="150000" tscDM:valign="bottom" tscDM:halign="center"><xmpDM:name><rdf:Alt><rdf:li xml:lang="en-US">{\rtf1 Hãy dừng lại một chút và suy nghĩ về kinh nghiệm giảng dạy của bạn.}</rdf:li></rdf:Alt></xmpDM:name></rdf:Description></rdf:li><rdf:li><rdf:Description xmpDM:duration="4867" xmpDM:startTime="153000" tscDM:valign="bottom" tscDM:halign="center"><xmpDM:name><rdf:Alt><rdf:li xml:lang="en-US">{\rtf1 Bạn đã bao giờ dạy một bài học, cảm thấy hài lòng, sau đó kiểm tra sự hiểu biết và \par phát hiện ra rằng học sinh nhớ rất ít hoặc không nhớ gì về nội dung chưa?}</rdf:li></rdf:Alt></xmpDM:name></rdf:Description></rdf:li><rdf:li><rdf:Description xmpDM:duration="7967" xmpDM:startTime="157867" tscDM:valign="bottom" tscDM:halign="center"><xmpDM:name><rdf:Alt><rdf:li xml:lang="en-US">{\rtf1 Đây có thể là kết quả của quá tải nhận thức.}</rdf:li></rdf:Alt></xmpDM:name></rdf:Description></rdf:li><rdf:li><rdf:Description xmpDM:duration="24867" xmpDM:startTime="165833" tscDM:valign="bottom" tscDM:halign="center"><xmpDM:name><rdf:Alt><rdf:li xml:lang="en-US">{\rtf1 Hãy lắng nghe từ một giáo viên giàu kinh nghiệm, Mark, về cảm giác của anh ấy \par sau khi nhận ra học sinh đang gặp phải quá tải nhận thức.}</rdf:li></rdf:Alt></xmpDM:name></rdf:Description></rdf:li><rdf:li><rdf:Description xmpDM:duration="11133" xmpDM:startTime="190700" tscDM:valign="bottom" tscDM:halign="center"><xmpDM:name><rdf:Alt><rdf:li xml:lang="en-US">{\rtf1 Tôi là một giáo viên, điều này làm bạn cảm thấy hơi thất vọng. Tôi không thể không \par cảm thấy thất vọng vì tôi làm trong nghề mà việc học là}</rdf:li></rdf:Alt></xmpDM:name></rdf:Description></rdf:li><rdf:li><rdf:Description xmpDM:duration="8333" xmpDM:startTime="201833" tscDM:valign="bottom" tscDM:halign="center"><xmpDM:name><rdf:Alt><rdf:li xml:lang="en-US">{\rtf1 điều tối quan trọng đối với tôi. Và vì vậy, khi học sinh không học được, tôi cảm thấy \par bài giảng đã thất bại.}</rdf:li></rdf:Alt></xmpDM:name></rdf:Description></rdf:li><rdf:li><rdf:Description xmpDM:duration="5933" xmpDM:startTime="210167" tscDM:valign="bottom" tscDM:halign="center"><xmpDM:name><rdf:Alt><rdf:li xml:lang="en-US">{\rtf1 Ở một số khía cạnh, tôi đã không làm tròn trách nhiệm với học sinh của mình.\par }</rdf:li></rdf:Alt></xmpDM:name></rdf:Description></rdf:li></rdf:Seq>
